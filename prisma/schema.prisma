generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Notes {
  id      Int    @id @default(autoincrement())
  title   String
  user_id String
  user    Users  @relation(fields: [user_id], references: [id])
}

model Users {
  id    String  @id @default(uuid())
  name  String  @unique
  notes Notes[]
}
